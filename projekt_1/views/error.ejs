<%- include('inc/header'); -%>

<div class="container">
    <div class="text-center mt-5">
        <% if (error.status === 404) { %>
            <div class="alert alert-warning" role="alert">
                <h1><b>404</b> - Podana strona nie istnieje</h1>
            </div>
        <% } else { %>
            <div class="alert alert-warning" role="alert">
                <h1>Wystąpił błąd: <%= error.status %></h1>
                <h2><%= message %></h2>
            </div>
        <% } %>
    </div>
</div>

<%- include('inc/footer'); -%>